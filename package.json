{
    "name": "keycloak-authenticator",
    "version": "0.1.0",
    "description": "keycloak authenticator",
    "repository": {},
    "author": "Sergio Sicari",
    "license": "MIT",
    "scripts": {
        "start": "npm run serve",
        "build": "npm run build-ts && npm run lint",
        "serve": "node lib/app.js",
        "watch-node": "nodemon lib/app.js",
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
        "test": "jest --forceExit --coverage --verbose",
        "watch-test": "npm run test -- --watchAll",
        "build-ts": "tsc",
        "watch-ts": "tsc -w",
        "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
        "debug": "npm run build && npm run watch-debug",
        "serve-debug": "nodemon --inspect lib/server.js",
        "watch-debug": "concurrently -k -p \"[{name}]\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
    },
    "dependencies": {
        "async": "^3.1.0",
        "body-parser": "^1.19.0",
        "class-transformer": "^0.3.1",
        "class-validator": "^0.11.1",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "errorhandler": "^1.5.1",
        "express": "^4.17.1",
        "express-flash": "0.0.2",
        "express-session": "^1.16.2",
        "express-validator": "^6.2.0",
        "keycloak-connect": "^9.0.2",
        "path": "^0.12.7",
        "qs": "^6.9.3",
        "reflect-metadata": "^0.1.13",
        "routing-controllers": "^0.8.1-alpha.2",
        "typedi": "^0.8.0",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@types/async": "^3.0.2",
        "@types/body-parser": "^1.17.1",
        "@types/chai": "^4.2.3",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.6",
        "@types/errorhandler": "^0.0.32",
        "@types/eslint": "^6.1.1",
        "@types/express": "^4.17.1",
        "@types/express-flash": "0.0.1",
        "@types/express-session": "^1.15.14",
        "@types/node": "^12.7.8",
        "@types/request": "^2.48.3",
        "@types/request-promise": "^4.1.44",
        "@typescript-eslint/eslint-plugin": "^2.27.0",
        "@typescript-eslint/parser": "^2.27.0",
        "chai": "^4.2.0",
        "eslint": "^6.8.0",
        "ts-node": "^8.4.1",
        "typescript": "^3.6.3"
    }
}
